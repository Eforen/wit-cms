<html>
  <head>
    <title><%= page.title %></title>
    <meta name='view' content='blog'>
    <link rel='canonical' href='<%= page.url %>'>
  </head>
  <body>

      <h1><%= page.title %></h1>

      <main>
      <% page.content.forEach(function(post) {%>
        <h2><%= post.title %></h2>
      <% })%>
      </main>

  </body>
</html>
